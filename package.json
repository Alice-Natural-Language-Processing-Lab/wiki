{
  "name": "wikijs",
  "description": "Wikipedia interface for node",
  "author": "Richard van der Dys",
  "version": "0.2.2",
  "keywords": [
    "wiki",
    "wikipedia",
    "interface"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.github.com/dijs/wiki/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">=0.10.4"
  },
  "main": "./dist/wiki.js",
  "dependencies": {
    "babel-polyfill": "^6.9.0",
    "isomorphic-fetch": "^2.2.1",
    "underscore": "^1.8.3",
    "wiki-infobox-parser-core": "0.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.10.2",
    "istanbul": "^1.0.0-alpha.2",
    "jsdoc": "^3.3.0-beta3",
    "minami": "^1.1.0",
    "mocha": "^2.2.4",
    "nock": "^8.0.0",
    "should": "^6.0.1",
    "should-promised": "^0.2.0"
  },
  "scripts": {
    "build": "babel src --out-dir dist --source-maps inline",
    "test": "babel-node ./node_modules/istanbul/lib/cli cover node_modules/.bin/_mocha",
    "docs": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "homepage": "https://github.com/dijs/wiki",
  "bugs": "https://github.com/dijs/wiki/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/dijs/wiki.git"
  }
}
